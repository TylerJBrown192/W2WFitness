{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": false,
        "object-literal-sort-keys": false,
        "quotemark": [
            true,
            "single"
        ],
        "no-consecutive-blank-lines": [
            true,
            2
        ],
        "max-line-length": [
            true,
            {
                "limit": 200,
                "ignore-pattern": "^import [^,]+ from |^export | implements"
            }
        ]
    },
    "rulesDirectory": []
}